#include <iostream>
#include <vector>
using namespace std; 
// Dutch National falg algoritham
void DNF(vector<int>& nums){
    int low = 0, mid = 0, high = nums.size() - 1;
    while(mid <= high){
        if(nums[mid] == 0){
            swap(nums[mid], nums[low]);
            mid++, low++;
        }
        else if (nums[mid] == 1) mid++;
        else{
            swap(nums[mid], nums[high]);
            high--;
        }
    }
}
void printvec(vector<int> &nums){
    for(int i = 0; i < nums.size(); i++){
        cout << nums[i] << " ";
    }
}

int main ()
{
    vector<int> nums = {1,0,0,1,0,2,1,2,0};
    DNF(nums);
    printvec(nums);
    return 0;
}
