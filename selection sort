#include <iostream>
#include <vector>
using namespace std; 
void SelectionSort(vector<int>&nums, int n){
    for(int i = 0; i < n - 1; i++){
        int smallestIndx = i;
        for(int j = i + 1; j < n; j++){
            if(nums[j] < nums[smallestIndx]){
                smallestIndx = j;
            }
        }
        /* In swap function
           you cannot swap nums[i] with just value 10,
           you can swap nums[i] with nums[smallestInd]
        */  
        swap(nums[i], nums[smallestIndx]);
    }
}

void PrintArray(vector <int> nums, int n){
    for(int i = 0; i < n; i++){
        cout << nums[i] << " ";
    }
    cout << endl;
}
int main ()
{   
    vector <int> nums = {12,33,11,4,3,15};
    int n = 6;
    SelectionSort(nums, n);
    PrintArray(nums, n);
    return 0;
}
